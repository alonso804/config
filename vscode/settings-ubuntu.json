{
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "w"],
      "commands": [":w"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":q"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "n"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<C-n>"],
      "commands": ["revealInExplorer"]
    }
  ],
  "vim.leader": "<space>",
  "editor.tabSize": 2,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "editor.fontSize": 13,
  "editor.bracketPairColorization.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "One Dark Pro Darker",
  "editor.fontFamily": "'Cascadia Code', 'CaskaydiaCove NF', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum', 'calt', 'ss01'",
  "C_Cpp.formatting": "vcFormat",
  "editor.lineNumbers": "relative",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "CaskaydiaCove NF Mono",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "thunder-client.codeSnippetLanguage": "js-axios",
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f"
  },
  "editor.wordWrap": "on",
  "diffEditor.wordWrap": "off",
  "editor.guides.indentation": true,
  "editor.guides.bracketPairs": false,
  "editor.inlineSuggest.enabled": true,
  "editor.renderWhitespace": "all",
  "sync.gist": "6dd64806999aca594c635e3de4deefa9",
  "settingsSync.ignoredExtensions": [],
  "redhat.telemetry.enabled": false,
  "sync.autoDownload": true,
  "sync.quietSync": true,
  "editor.tokenColorCustomizations": {
    "[One Dark Pro Darker]": {
      "variables": "#ABB2BF",
      "textMateRules": [
        {
          "scope": [
            "keyword.operator",
            "keyword.operator.arithmetic",
            "keyword.operator.assignment",
            "keyword.operator.comparison",
            "keyword.operator.logical",
            "keyword.operator.new"
          ],
          "settings": {
            "foreground": "#56B6C2"
          }
        },
        {
          "scope": [
            "comment",
            "keyword.control",
            "keyword.other",
            "storage.type.function.js",
            "storage.modifier.async.js",
            "storage.type.js"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        }
      ]
    }
  },
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "*.mutable": {
        "underline": false
      }
    },
    "[One Dark Pro Darker]": {
      "rules": {
        "property": "#E06C75",
        "variable": "#ABB2BF"
      }
    }
  },
  "diffEditor.renderSideBySide": false,
  "terminal.integrated.inheritEnv": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
  "window.zoomLevel": 1
}
